<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane minHeight="400.0" minWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="application.ChangeMovieController">
   <children>
      <Label layoutX="242.0" layoutY="35.0" text="Change Movie">
         <font>
            <Font name="American Typewriter" size="32.0" />
         </font>
      </Label>
      <TextField fx:id="newMovieName" layoutX="256.0" layoutY="122.0" prefHeight="26.0" prefWidth="190.0" />
      <TextField fx:id="newPosterURL" layoutX="256.0" layoutY="182.0" prefHeight="26.0" prefWidth="328.0" />
      <Label layoutX="140.0" layoutY="124.0" text="Movie Name:" />
      <Label layoutX="140.0" layoutY="183.0" text="Poster URL:" />
      <Button fx:id="movieOne" layoutX="30.0" layoutY="249.0" mnemonicParsing="false" onAction="#changeMovie" prefHeight="80.0" prefWidth="150.0" style="-fx-background-color: white; -fx-border-color: black;" text="Movie 1" />
      <Button fx:id="movieTwo" layoutX="195.0" layoutY="249.0" mnemonicParsing="false" onAction="#changeMovie" prefHeight="80.0" prefWidth="150.0" style="-fx-background-color: white; -fx-border-color: black;" text="Movie 2" />
      <Button fx:id="movieThree" layoutX="359.0" layoutY="249.0" mnemonicParsing="false" onAction="#changeMovie" prefHeight="80.0" prefWidth="150.0" style="-fx-background-color: white; -fx-border-color: black;" text="Movie 3" />
      <Button fx:id="movieFour" layoutX="523.0" layoutY="249.0" mnemonicParsing="false" onAction="#changeMovie" prefHeight="80.0" prefWidth="150.0" style="-fx-background-color: white; -fx-border-color: black;" text="Movie 4" />
   </children>
</AnchorPane>
